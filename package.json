{
  "name": "tailwindcss-intellisense",
  "displayName": "Tailwind CSS IntelliSense",
  "description": "Tailwind CSS Extension for VS Code",
  "version": "0.0.1",
  "author": "lightyen",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lightyen/vscode-tailwindcss-twin.git"
  },
  "publisher": "lightyen",
  "categories": [],
  "keywords": [
    "tailwind",
    "tailwindcss",
    "intellisense",
    "autocomplete",
    "vscode"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "galleryBanner": {
    "color": "#121b2d"
  },
  "icon": "media/icon.png",
  "activationEvents": [
    "*"
  ],
  "main": "./out/client/extension.js",
  "scripts": {
    "test": "jest",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.webpack.json webpack --config webpack.config.ts",
    "watch": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.webpack.json webpack --config webpack.config.ts --watch",
    "vscode:prepublish": "yarn build",
    "package": "vsce package --yarn"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-webpack-plugin": "^2.4.0",
    "fork-ts-checker-webpack-plugin": "^6.0.3",
    "jest": "^26.6.3",
    "js-yaml-loader": "^1.2.2",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "ts-paths-resolve-plugin": "^5.2.2",
    "typescript": "^4.1.2",
    "vsce": "^1.81.1",
    "vscode-languageclient": "^6.1.3",
    "vscode-languageserver": "^6.1.1",
    "vscode-languageserver-textdocument": "^1.0.1",
    "vscode-nls": "^5.0.0",
    "vscode-nls-dev": "^3.3.2",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {
    "postcss": "^8.1.13",
    "tailwindcss": "^2.0.1"
  },
  "contributes": {
    "configuration": {
      "title": "Tailwind CSS IntelliSense",
      "properties": {
        "tailwindcss.colorDecorators": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null
        },
        "tailwindcss.links": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null
        },
        "tailwindcss.twin": {
          "type": "boolean",
          "default": true
        },
        "tailwindcss.validate": {
          "type": "boolean",
          "default": true
        }
      }
    },
    "commands": []
  }
}
