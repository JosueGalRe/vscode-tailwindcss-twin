{
  "name": "tailwindcss-intellisense-twin",
  "displayName": "Tailwind Twin IntelliSense",
  "description": "Tailwind VSCode Extension for twin.macro",
  "version": "0.9.4",
  "author": "lightyen",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lightyen/vscode-tailwindcss-twin.git"
  },
  "publisher": "lightyen",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "tailwind",
    "tailwindcss",
    "autocomplete",
    "intellisense",
    "twin",
    "twin.macro",
    "css-in-js",
    "react",
    "babel-macro",
    "javascript",
    "typescript"
  ],
  "engines": {
    "vscode": "^1.56.1"
  },
  "galleryBanner": {
    "color": "#191A1F",
    "theme": "dark"
  },
  "icon": "media/icon.png",
  "activationEvents": [
    "workspaceContains:**/{*.js,*.jsx,*.ts,*.tsx}"
  ],
  "main": "./dist/extension",
  "scripts": {
    "test": "jest",
    "doit": "yarn upgrade-interactive --latest",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.webpack.json webpack --config webpack.config.ts",
    "watch": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.webpack.json webpack --config webpack.config.ts --watch",
    "vscode:prepublish": "yarn build",
    "format": "prettier --write \"**/*.{ts,json,yml}\"",
    "check-format": "prettier --check \"**/*.{ts,json,yml}\"",
    "package": "vsce package --yarn"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-typescript": "7.15.0",
    "@types/jest": "27.0.2",
    "@types/node": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-webpack-plugin": "3.0.1",
    "fork-ts-checker-webpack-plugin": "6.3.3",
    "jest": "27.2.1",
    "js-yaml-loader": "1.2.2",
    "prettier": "2.4.1",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "ts-paths-resolve-plugin": "5.3.12",
    "vsce": "1.99.0",
    "webpack": "5.53.0",
    "webpack-cli": "4.8.0",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "postcss": "8.3.6",
    "tailwindcss": "2.2.15",
    "typescript": "4.4.3"
  },
  "contributes": {
    "configurationDefaults": {
      "[javascript]": {
        "editor.quickSuggestions": {
          "strings": true
        }
      },
      "[typescript]": {
        "editor.quickSuggestions": {
          "strings": true
        }
      },
      "[javascriptreact]": {
        "editor.quickSuggestions": {
          "strings": true
        }
      },
      "[typescriptreact]": {
        "editor.quickSuggestions": {
          "strings": true
        }
      }
    },
    "grammars": [
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.twin",
        "path": "./syntaxes/twin.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.twin-theme",
        "path": "./syntaxes/theme.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.twin-screen",
        "path": "./syntaxes/screen.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.twin-prop",
        "path": "./syntaxes/twProp.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.cs-prop",
        "path": "./syntaxes/csProp.tmLanguage.json"
      }
    ],
    "configuration": {
      "title": "Tailwind Twin IntelliSense",
      "properties": {
        "tailwindcss.colorDecorators": {
          "description": "%ext.settings.colorDecorators%",
          "type": [
            "boolean",
            "null"
          ],
          "default": null
        },
        "tailwindcss.references": {
          "description": "%ext.settings.references%",
          "type": "boolean",
          "default": true
        },
        "tailwindcss.diagnostics": {
          "description": "%ext.settings.diagnostics%",
          "type": "object",
          "properties": {
            "enabled": {
              "description": "%ext.settings.diagnostics.enabled%",
              "type": "boolean",
              "default": true
            },
            "conflict": {
              "type": "string",
              "enum": [
                "none",
                "loose",
                "strict"
              ],
              "default": "strict",
              "enumDescriptions": [
                "%ext.settings.diagnostics.conflict.none%",
                "%ext.settings.diagnostics.conflict.loose%",
                "%ext.settings.diagnostics.conflict.strict%"
              ]
            },
            "emptyClass": {
              "type": "boolean",
              "default": true
            },
            "emptyGroup": {
              "type": "boolean",
              "default": true
            },
            "emptyCssProperty": {
              "type": "boolean",
              "default": true
            }
          },
          "default": {
            "enabled": true,
            "conflict": "strict",
            "emptyClass": true,
            "emptyGroup": true,
            "emptyCssProperty": true
          }
        },
        "tailwindcss.preferVariantWithParentheses": {
          "type": "boolean",
          "default": false
        },
        "tailwindcss.fallbackDefaultConfig": {
          "type": "boolean",
          "default": true
        },
        "tailwindcss.enabled": {
          "description": "%ext.settings.enabled%",
          "type": "boolean",
          "default": true
        },
        "tailwindcss.jsxPropImportChecking": {
          "description": "%ext.settings.jsxPropImportChecking%",
          "type": "boolean",
          "default": true
        },
        "tailwindcss.rootFontSize": {
          "type": [
            "number",
            "boolean"
          ],
          "default": true
        }
      }
    },
    "commands": []
  }
}
